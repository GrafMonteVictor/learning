<web-app>

    <servlet>
        <servlet-name>Print-Servlet</servlet-name>
        <servlet-class>PrintServlet</servlet-class>
        <init-param>
            <param-name>jdbc-driver</param-name>
            <param-value>sun.jdbc.odbc.JdbcOdbcDriver</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>Print-Servlet</servlet-name>
        <url-pattern>/print</url-pattern>
    </servlet-mapping>

</web-app>
